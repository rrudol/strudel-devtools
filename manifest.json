{
  "name": "Strudel Devtools",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Devtools for Strudel.js framework",
  "permissions": [
    "storage"
  ],
  "homepage_url": "http://strudeljs.org",
  "devtools_page": "index.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "node_modules/*",
    "build/*"
  ]
}